{
	"name": "rollcalapi",
	"version": "18.12.01",
	"description": "Roll-Cal.com API",
	"private": true,
	"author": {
		"name": "Kevin Joseph",
		"url": "http://kjbranded.com/"
	},
	"license": "GPL-3.0",
	"homepage": "http://www.roll-cal.com/",
	"repository": "github:kj-joseph/rollcal",
	"bugs": {
		"url": "https://github.com/kj-joseph/rollcal/issues"
	},
	"scripts": {
		"build": "npm-run-all lint clearBuildDir 'webpack:local -- --env.NOSTART'",
		"build:dev": "npm-run-all lint webpack:dev",
		"build:stage": "npm-run-all lint webpack:stage",
		"build:prod": "npm-run-all lint webpack:prod",
		"clearBuildDir": "mkdir -p build && rm -rf build/*",
		"lint": "tslint -p ./jsconfig.json -c ./tslint.json",
		"precommit-msg": "echo 'Pre-commit checks...' && exit 0",
		"start": "npm run webpack:local -- --watch",
		"webpack:dev": "webpack --config webpack.dev.js --mode development",
		"webpack:local": "webpack --config webpack.local.js --mode development",
		"webpack:prod": "webpack --config webpack.prod.js --mode production",
		"webpack:stage": "webpack --config webpack.stage.js --mode production"
	},
	"dependencies": {
		"@google/maps": "^0.5.5",
		"axios": "^0.18.1",
		"babel-polyfill": "^6.26.0",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"express": "^4.17.1",
		"express-mysql-session": "^2.1.0",
		"express-session": "^1.16.2",
		"morgan": "^1.9.1",
		"multer": "^1.4.2",
		"mysql": "^2.17.1",
		"nodemailer": "^4.7.0",
		"typescript": "^3.5.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-typescript": "^7.3.3",
		"@types/express": "^4.17.0",
		"@types/express-session": "^1.15.13",
		"@types/googlemaps": "^3.37.0",
		"@types/mysql": "^2.15.6",
		"@types/node": "^10.14.13",
		"@types/webpack-env": "^1.14.0",
		"babel-loader": "^8.0.6",
		"ical-generator": "^1.7.2",
		"moment": "^2.29.4",
		"nodemon": "^1.19.1",
		"npm-run-all": "^4.1.5",
		"terser-webpack-plugin": "^1.3.0",
		"tslint": "^5.18.0",
		"uglifyjs-webpack-plugin": "^2.1.3",
		"webpack": "^4.38.0",
		"webpack-cli": "^3.3.6",
		"webpack-merge": "^4.2.1",
		"webpack-shell-plugin-next": "^0.6.4"
	}
}
