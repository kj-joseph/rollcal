{
  "name": "rollcalapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all lint clearBuildDir 'webpack:local -- --env.NOSTART'",
    "build:dev": "npm-run-all lint webpack:dev",
    "build:stage": "npm-run-all lint webpack:stage",
    "build:prod": "npm-run-all lint webpack:prod",
    "clearBuildDir": "mkdir -p build && rm -rf build/*",
    "lint": "tslint -p ./jsconfig.json",
    "start": "npm run webpack:local -- --watch",
    "webpack:dev": "webpack --config webpack.dev.js --mode development",
    "webpack:local": "webpack --config webpack.local.js --mode development",
    "webpack:prod": "webpack --config webpack.prod.js --mode production",
    "webpack:stage": "webpack --config webpack.stage.js --mode production"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-mysql-session": "^2.0.1",
    "express-session": "^1.15.6",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "nodemailer": "^4.6.8",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/express": "^4.16.0",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.11.7",
    "@types/webpack-env": "^1.13.6",
    "babel-loader": "^8.0.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "terser-webpack-plugin": "^1.1.0",
    "tslint": "^5.11.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-shell-plugin-next": "^0.6.4"
  }
}
